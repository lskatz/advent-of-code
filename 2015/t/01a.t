#!/usr/bin/env perl

use strict;
use warnings;
use Data::Dumper;
use Test::More tests=>2;

subtest "Test $0" => sub{
  my $data = "
(())
()()
(((
(()(()(
))(((((
())
))(
)))
)())())
  ";

  my $floors = calculateFloors($data);
  subtest 'individual items' => sub{
    my @exp = qw(0 0 3 3 3 -1 -1 -3 -3);
    plan tests=>scalar(@exp);

    for(my $i=0; $i<@exp; $i++){
      is($$floors[$i], $exp[$i]);
    }
  };
};

subtest "Real $0" => sub{
  local $/ = undef;
  my $data = <DATA>;
  $data =~ s/^\s+|\s+$//g; # whitespace trim on each line
  my $floors = calculateFloors($data);
  is($$floors[0], 138);
};

sub calculateFloors{
  my($data) = @_;
  my @floors;

  for my $line(split(/\n/, $data)){
    next if($line =~ /^\s*$/);
    chomp($line);

    my $floor = 0;
    while($line =~ /([\(\)])/g){
      if($1 eq '('){
        $floor++;
      }
      elsif($1 eq ')'){
        $floor--;
      }
    }
    push(@floors, $floor);
  }

  return \@floors;
}

