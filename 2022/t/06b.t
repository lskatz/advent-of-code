#!/usr/bin/env perl

use strict;
use warnings;
use Data::Dumper;
use Test::More tests=>2;
use List::Util qw/uniq/;

subtest "Test $0" => sub{
  my @data = qw(
  mjqjpqmgbljsphdztnvjfqwrcgsmlb
  bvwbjplbgvbhsrlpgdmjqwftvncz
  nppdvjthqldpwncqszvftbrmjlhg
  nznrnfrfntjfmvfwmzdfjlvtqnbhcprsg
  zcfzfwzzqfrljwzlrfnpqdbhtmscgvjw
  );

  my @exp = (19,23,23,29,26);

  my $obs = firstUnique(\@data);
  is_deeply($obs, \@exp);
};

subtest "Real $0" => sub{
  my $data = <DATA>;
  my $obs = firstUnique([$data]);
  is($$obs[0], 2950);
};

sub firstUnique{
  my($data) = @_;

  # How many characters to check for uniqueness
  my $numChars = 14;

  my @firstUnique;

  for my $datastreamBuffer(@$data){
    for(my $i=0;$i<length($datastreamBuffer)-$numChars;$i++){
      my @char = split(//,
        substr($datastreamBuffer, $i, $numChars)
      );
      if(scalar(uniq(@char)) == scalar(@char)){
        push(@firstUnique, $i+$numChars);
        last;
      }
    }
  }

  return \@firstUnique;
}

__DATA__

