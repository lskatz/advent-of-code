#!/usr/bin/env perl

use strict;
use warnings;
use Data::Dumper;
use Test::More tests=>2;
use List::Util qw/sum max/;

subtest "Test $0" => sub{
  my @data = qw(
    vJrwpWtwJgWrhcsFMMfFFhFp
    jqHRNqRjqzjGDLGLrsFMfFZSrLrFZsSL
    PmmdzqPrVvPwwTWBwg
    wMqvLMZHhHMvwLHjbvcjnnSBnvTQFn
    ttgJtRGJQctTZtZT
    CrZsJsPPZsGzwwsLwLmpwMDw
  );

  my @exp = (16, 38, 42, 22, 20, 19);

  my $priority = priorityContent(\@data);
  is_deeply($priority, \@exp);
  is(sum(@$priority), 157, "sum");
};

subtest "Real $0" => sub{
  my @data;
  while(<DATA>){
    chomp;
    next if /^\s*$/;
    push(@data, $_);
  }
  my $priority = priorityContent(\@data);
  is(sum(@$priority), 8394);
};

sub priorityContent{
  my($data) = @_;

  my @priority;

  for(my $i=0;$i<@$data;$i++){
    my $len = length($$data[$i]);
    my $half = $len/2;
    my @compartment = ($$data[$i] =~ /.{$half}/g);

    my $letterInBoth = "";
    for my $letter(split(//, $compartment[0])){
      if(-1 != index($compartment[1], $letter)){
        $letterInBoth = $letter;
        last;
      }
    }

    my $offset = ord("a");
    if($letterInBoth =~ /[A-Z]/){
      $offset = ord("A") - 26;
    }

    push(@priority, (ord($letterInBoth) - $offset+1));
  }
  return \@priority;
}

